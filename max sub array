class Solution:
    def maxSubArray(self,nums):
        currsum=[0]
        maxi=nums[0]
        for num in nums:
            currsum+=num
            maxi=max(currsum,maxi)
            currsum=max(currsum,0)
        return maxi
ob=Solution()
nums=[-2,1,-3,4,-1,2,1,5,-4]
print(ob.maxSubArray(nums))    
