class Solution:
    def rotate(self,nums,k):
        n=len(nums)
        k=k%n
        def rev(s,e):
            while s<e:
                nums[s],nums[e]=nums[e],nums[s]
                s+=1
                e-=1
        rev(0,n-1)
        rev(0,k-1)
        rev(k,n-1)
        return nums
ob=Solution()
nums=[1,2,3,4,5,6,7]
k=3
print(ob.rotate(nums,k))
